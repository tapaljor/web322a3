{
    "version": 2,
    "builds": [
        {
        "src": "server.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "files": [
    "users.json"
  ]
}
